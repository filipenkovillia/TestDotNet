@model System.Data.DataTable
@{
    ViewBag.Title = "Phone Numbers";
}
<h2>@ViewBag.Title.</h2>

<table class="table table-bordered table-striped">
    <tr>
        <th>Phone Number ID</th>
        <th>Phone Number</th>
        <th>Is Active</th>
        <th>User ID</th>
        <th>Actions</th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][0]</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>
                <a href="@Url.Action("Edit", "PhoneNumber", new { @id = Model.Rows[i][0] })">Edit</a>
                <a href="@Url.Action("Delete", "PhoneNumber", new { @id = Model.Rows[i][0] })">Delete</a>
            </td>
        </tr>
    }
</table>

<a href="@Url.Action("Create", "PhoneNumber")">Add Phone Number</a>

